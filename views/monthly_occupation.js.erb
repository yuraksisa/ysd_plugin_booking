require(['YSDGui','jquery', 
         'jquery.ui', 'jquery.ui.datepicker-es','jquery.fixedtable','datejs'], function(YSDGui, $) {

  monthlyOccupationModel = {

  };

  monthlyOccupationController = {
     monthChanged: function() {
         monthlyOccupationView.navigate();
     },
     yearChanged: function() {
         monthlyOccupationView.navigate();
     },
     infoChanged: function() {
         monthlyOccupationView.navigate();
     }
  };

  monthlyOccupationView = {
	 init: function() {
	     $('#month').bind('change', function() {
           monthlyOccupationController.monthChanged();
       });
       $('#year').bind('change', function() {
           monthlyOccupationController.yearChanged();
       });
       $('#info').bind('change', function() {
           monthlyOccupationController.infoChanged();
       });
    },
    navigate: function() {
        window.location.href='/admin/booking/monthly-occupation?month='+$('#month').val()+'&year='+$('#year').val()+'&info='+$('#info').val();
    }
  };

  monthlyOccupationView.init();
});