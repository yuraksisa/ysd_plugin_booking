
                     <table class="table">
                        <thead class="table-header">
                          <tr>
                            <th class="table-cell left_text" scope="col" style="width: 120px"><%=t.booking_management.form.booking_resources.item%></th>
                            <% if booking_item_family.named_resources %>
                            <th class="table-cell left_text" scope="col" style="width: 280px"><%=t.booking_management.form.booking_resources.user%></th>
                            <th class="table-cell left_text" scope="col"><%=t.booking_management.form.booking_resources.document_id%></th>
                            <th class="table-cell left_text" scope="col"><%=t.booking_management.form.booking_resources.phone%></th>
                            <th class="table-cell left_text" scope="col"><%=t.booking_management.form.booking_resources.email%></th>            
                            <% end %>
                            <% if booking_item_family.height %>                            
                            <th class="table-cell centered_text" scope="col"><%=t.booking_management.form.booking_resources.height%></th>
                            <% end %>    
                            <% if booking_item_family.weight %>                       
                            <th class="table-cell centered_text" scope="col"><%=t.booking_management.form.booking_resources.weight%></th>
                            <% end %>                            
                            <th class="table-cell centered_text" scope="col"  style="width: 120px"><%=t.booking_management.form.booking_resources.resource%></th>
                            <th class="table-cell" scope="col"></th>
                          </tr>
                        </thead>
                        <tbody class="table-tbody">
                         <%% for (var lineIdx=0; lineIdx < entity.booking_lines.length; lineIdx++) { %>
                            <%% for (var resourceIdx=0; resourceIdx < entity.booking_lines[lineIdx].booking_line_resources.length; resourceIdx++) { %>
                            <tr class="table-row">
                               <td class="table-cell left_text smaller_text more_contrast_text" style="position:relative; top: 10px"><%%= entity.booking_lines[lineIdx].item_description %></td>
                               <% if booking_item_family.named_resources %>
                               <td class="table-cell left_text">
                                 <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>                                 
                                   <input class="fieldcontrol resource_user_name" type="text" maxlength="80" size="8" name="resource_user_name" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_name%>"/>
                                   <input class="fieldcontrol resource_user_surname" type="text" maxlength="80" size="10" name="resource_user_surname" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_surname %>"/>
                                 </form>   
                               </td>
                               <td class="table-cell left_text">
                                 <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_document_id" type="text" maxlength="50" size="10" name="resource_user_document_id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_document_id%>"/>
                                 </form>                                                         
                               </td>
                               <td class="table-cell left_text">
                                 <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_phone" type="text" maxlength="15" size="10" name="resource_user_phone" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_phone%>"/>
                                 </form>                                                     
                               </td>
                               <td class="table-cell left_text">
                                 <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_email" type="text" maxlength="40" size="15" name="resource_user_email" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_email%>"/>
                                 </form>                                                           
                               </td>                                                               
                               <% end %>            
                               <% if booking_item_family.height %>               
                               <td class="table-cell centered_text">
                                  <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                    <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                    <input class="fieldcontrol customer_height" type="text" maxlength="3" size="4" name="customer_height" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].customer_height%>"/>
                                  </form>
                               </td>
                               <% end %>
                               <% if booking_item_family.weight %>
                               <td class="table-cell centered_text">
                                  <form data-name="height" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                    <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                    <input class="fieldcontrol customer_weight" type="text" maxlength="3" size="4" name="customer_weight" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].customer_weight%>"/>
                                  </form>
                               </td>
                               <% end %>
                               <td class="table-cell centered_text">
                                 <div>
                                   <%% if (entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].booking_item_reference == null) { %>
                                     <div class="more_contrast_text smaller_text" style="display:inline-block; position: relative; top: 6px">NO ASIGNADO</div>
                                   <%% } else { %>
                                     <form data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>" style="display: inline-block">
                                       <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                       <button type="button" name="booking_item_reference_detail_button_<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>" class="btn btn-default booking_item_reference_detail"><%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].booking_item_reference%></button>
                                     </form>
                                   <%% } %>
                                 </div>
                               </td>
                               <td class="table-cell"></td>
                            </tr>
                            <%% if (entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].pax == 2) { %>
                            <tr class="table-row">
                               <td class="table-cell"></td>
                               <% if booking_item_family.named_resources %>
                               <td class="table-cell left_text">
                                 <form class="top-margin" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>                                 
                                   <input class="fieldcontrol resource_user_2_name" type="text" maxlength="80" size="8" name="resource_user_2_name" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_2_name%>"/>
                                   <input class="fieldcontrol resource_user_2_surname" type="text" maxlength="80" size="10" name="resource_user_2_surname" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_2_surname %>"/>
                                 </form>                                        
                               </td>
                               <td class="table-cell left_text">
                                 <form class="top-margin" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_2_document_id" type="text" maxlength="50" size="10" name="resource_user_2_document_id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_2_document_id%>"/>
                                 </form>                                                        
                               </td>
                               <td class="table-cell left_text">
                                 <form class="top-margin" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_2_phone" type="text" maxlength="15" size="10" name="resource_user_2_phone" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_2_phone%>"/>
                                 </form>                    
                               </td>
                               <td class="table-cell left_text">
                                 <form class="top-margin" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                   <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                   <input class="fieldcontrol resource_user_2_email" type="text" maxlength="40" size="15" name="resource_user_2_email" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].resource_user_2_email%>"/>
                                 </form>                                
                               </td>                                                               
                               <% end %>            
                               <% if booking_item_family.height %>               
                               <td class="table-cell centered_text">
                                  <form class="top-margin" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                    <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                    <input class="fieldcontrol customer_2_height" type="text" maxlength="3" size="4" name="customer_2_height" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].customer_2_height%>"/>
                                  </form>
                               </td>
                               <% end %>
                               <% if booking_item_family.weight %>
                               <td class="table-cell centered_text">
                                  <form class="top-margin" data-name="height" data-lineindex="<%%=lineIdx%>" data-resourceindex="<%%=resourceIdx%>">
                                    <input type="hidden" name="id" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].id%>"/>
                                    <input class="fieldcontrol customer_2_weight" type="text" maxlength="3" size="4" name="customer_2_weight" value="<%%=entity.booking_lines[lineIdx].booking_line_resources[resourceIdx].customer_2_weight%>"/>
                                  </form>                                           
                               </td>
                               <% end %>
                               <td class="table-cell"></td>
                               <td class="table-cell"></td>
                            </tr>
                            <%% } %>

                            <%% } %>
                          <%% } %>
                        </tbody>
                      </table>

                      <div style="position: relative; left: 10px">

                        <form name="planning" class="form-inline">
                          <input type="hidden" name="id" value="<%%=entity.id%>"/>
                          <div class="form-group">
                            <label for="planning_color">Color en el planning</label>&nbsp;
                            <input type="color" name="planning_color" value="<%%=entity.planning_color%>" style="width:50px" class="form-control"/>
                          </div>
                          <div class="form-group">
                            <button class="btn btn-default update-entity-button action-button entity-management-button" type="submit"><%=t.entitymanagement.update%></button>
                          </div>
                        </form>


                      </div>
