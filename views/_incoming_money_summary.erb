<h3><%=t.booking_incoming.title%></h3>
<div class="container_12">
  <div class="grid_8">
    <div id="booking_statics">
      <canvas id="incoming_money_graphic" width="640" height="500"></canvas>
    </div>
  </div>
  <div class="grid_4">
    <div id="incoming_money_data" class="guiblock smaller_text centered_text">
    </div>
  </div>
</div>
<script type="text/template" id="booking_incoming_table">
 <div class="table-container">
    <table id="elements_table" class="table">
       <thead class="table-header">
         <tr>
           <th class="table-header-title centered_text" scope="col" style="width:20%"><%=t.booking_incoming.period%></th>
           <th class="table-header-title right_text right-margin" scope="col" style="width:40%"><%=t.booking_incoming.total%></th>
         </tr>
       </thead>
       <tbody id="elements_tbody" class="table-tbody">  
         <%% for (item in data) { %>          
         <tr class="table-row"> 
           <td class="table-cell"><%%=item%></td>
           <td class="table-cell right_text right-margin"><%%=data[item].total%></td>
         </tr>
         <%% } %>
       </tbody>
    </table>
</script>